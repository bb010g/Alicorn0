let Result = lambda (T, E)
	enum
		Ok(x : T)
		Err(e : E)

let switchtest_ascribed = lambda (x : Result(host-number, host-number))
	switch x
		Ok(x) -> x
		Err(a) -> -1

switchtest_ascribed(mk Ok(5))
switchtest_ascribed(mk Er(3))
